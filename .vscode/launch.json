  "version": "0.2.0",
  "configurations": [
    {
      "name": "Debug",
      "cwd": "${workspaceRoot}",
      "executable": "./out/main.elf",
      "request": "launch",
      "type": "cortex-debug",
      "servertype": "qemu",
      "serverpath": "%USERPROFILE%\\AppData\\Roaming\\xPacks\\qemu-arm\\xpack-qemu-arm-7.1.0-1\\bin\\qemu-system-gnuarmeclipse.exe",
